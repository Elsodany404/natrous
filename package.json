    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@exortek/express-mongo-sanitize": "^1.1.1",
    "@m-nasser-m/paymob-sdk-eg": "^0.2.11",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "core-js": "^3.46.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-xss-sanitizer": "^2.0.1",
    "helmet": "^8.1.0",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "mongoose": "^8.19.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "pug": "^3.0.3",
    "sharp": "^0.34.4",
    "slugify": "^1.6.6",
