{
  "name": "natrous",
  "version": "1.0.0",
  "description": "Natrous App - Express, Pug, MongoDB, Parcel",
  "type": "module",
  "source": "js/index.js",
  "scripts": {
    "start": "nodemon server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "watch:js": "parcel watch js/index.js --dist-dir public/js",
